{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": [
		"expo/tsconfig.base",
		"@jlrickert/knutjs-tsconfig/base.json"
	],
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true
		// "module": "ESNext"
		// "typeRoots": [
		// 	"./node_modules/@types",
		// 	"./node_modules/@testing-library/react-native/extend-expect.d.ts"
		// ],
		// "types": [
		// 	"./node_modules/@testing-library/react-native/extend-expect.d.ts"
		// ]
	},
	"include": [
		"./src/**.tsx?",
		"./setup-jest.ts",
		".expo/types/**/*.ts",
		"expo-env.d.ts"
	],
	"exclude": [
		"babel.config.cjs",
		"metro.config.cjs"
	]
}
