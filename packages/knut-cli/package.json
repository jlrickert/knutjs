{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@jlrickert/knutjs-cli",
	"version": "0.1.0",
	"bin": {
		"knut": "./dist/cli.js"
	},
	"main": "./dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"module": "./dist/index.mjs",
			"types": "./dist/index.d.ts",
			"require": "./dist/index.js"
		},
		"./*": {
			"module": "./dist/*.mjs",
			"types": "./dist/*.d.ts",
			"require": "./dist/*.js"
		}
	},
	"files": [
		"dist",
		"package.json"
	],
	"scripts": {
		"build": "cross-env NODE_ENV=production tsup",
		"clean": "rimraf dist",
		"dev": "tsup --watch",
		"typecheck": "tsc --noEmit",
		"lint": "eslint src/",
		"test": "vitest",
		"prepare": "pnpm run build"
	},
	"devDependencies": {
		"@jlrickert/knutjs-tsconfig": "workspace:*",
		"@types/node": "^20.10.4",
		"commander": "^11.1.0",
		"cross-env": "^7.0.3",
		"rimraf": "^5.0.5",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3",
		"vitest": "^1.0.4"
	},
	"dependencies": {
		"@jlrickert/knutjs-core": "workspace:*"
	}
}
