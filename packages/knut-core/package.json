{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@jlrickert/knutjs-core",
	"version": "0.5.3",
	"license": "Apache-2.0",
	"types": "./dist/index.d.ts",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist",
		"src",
		"kegSchema.json",
		"knutSchema.json",
		"package.json"
	],
	"sideEffects": false,
	"scripts": {
		"build:tsc": "pnpm run clean && cross-env NODE_ENV=production tsc --project tsconfig.build.json && pnpm run typecheck",
		"clean:tsc": "tsc --build --clean",
		"dev:tsc": "pnpm run clean && tsc --build && tsc --build --watch",
		"dev": "tsup --watch",
		"build": "cross-env NODE_ENV=production tsup",
		"clean": "rimraf dist",
		"typecheck": "tsc --noEmit",
		"lint": "eslint ./src",
		"test": "vitest --dom",
		"prepublishOnly": "pnpm run build"
	},
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@jlrickert/knutjs-tsconfig": "workspace:*",
		"@types/node": "^20.10.4",
		"cross-env": "^7.0.3",
		"happy-dom": "^13.6.2",
		"rimraf": "^5.0.5",
		"tsup": "^8.1.0",
		"typescript": "^5.5.2",
		"vite": "^5.0.10",
		"vitest": "^1.0.4"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.474.0",
		"@types/mdast": "^4.0.3",
		"effect": "^3.4.0",
		"fp-ts": "^2.16.2",
		"fuse.js": "^7.0.0",
		"mdast-util-from-markdown": "^2.0.0",
		"mdast-util-gfm": "^3.0.0",
		"mdast-util-math": "^3.0.0",
		"mdast-util-to-markdown": "^2.1.0",
		"micromark": "^4.0.0",
		"micromark-extension-gfm": "^3.0.0",
		"remark": "^15.0.1",
		"remark-gfm": "^4.0.0",
		"remark-math": "^6.0.0",
		"tiny-invariant": "^1.3.1",
		"yaml": "^2.3.4"
	}
}
